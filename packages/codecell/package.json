{
  "name": "@betteridea/codecell",
  "repository": {
    "type": "git",
    "url": "https://github.com/betteridea-dev/ide/tree/main/packages/codecell"
  },
  "private": false,
  "version": "1.0.1",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
